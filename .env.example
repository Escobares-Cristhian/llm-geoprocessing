# === Choose the LLM provider to use: openai, gemini, ollama ===
GEO_LLM_PROVIDER=gemini
# Choose the model to use for the selected LLM provider
GEO_LLM_MODEL=gemini-3-flash-preview

# === Choose the provider(s) you actually use ===
OPENAI_API_KEY=replace_me
GEMINI_API_KEY=replace_me

# === PostGIS ===
POSTGIS_ENABLED=true
POSTGIS_HOST=localhost
POSTGIS_PORT=5432
POSTGIS_DB=geollm
POSTGIS_USER=geollm
POSTGIS_PASSWORD=geollm
POSTGIS_SCHEMA=public
POSTGIS_TABLE_PREFIX=gee_output_

# === Ollama ===
OLLAMA_MODEL=gpt-oss:120b
OLLAMA_NUM_CTX="131072"
OMP_NUM_THREADS=12
OLLAMA_NUM_GPU_LAYERS=1
OLLAMA_BASE_URL=http://localhost:11434
